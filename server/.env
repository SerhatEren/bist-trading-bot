# Server Configuration
PORT=3001
NODE_ENV=development
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info

# ALGOLAB API Configuration
ALGOLAB_API_URL=https://api.algolab.com.tr
ALGOLAB_API_KEY=your_api_key_here
ALGOLAB_API_SECRET=your_api_secret_here
ALGOLAB_USERNAME=your_username_here
ALGOLAB_PASSWORD=your_password_here

# Mock API Setting - Set to true to use mock API instead of real API
USE_MOCK_API=true 