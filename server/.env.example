# Server Configuration
PORT=3000
NODE_ENV=development

# JWT Authentication
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# ALGOLAB API Configuration
ALGOLAB_API_URL=https://api.algolab.com.tr/api/v1
ALGOLAB_API_KEY=your_api_key_here
ALGOLAB_API_SECRET=your_api_secret_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=15000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info 